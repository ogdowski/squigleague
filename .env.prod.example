# .env.prod.example
# Production environment configuration
# Copy this to .env.prod and fill in your actual values
# IMPORTANT: Keep this file secure and never commit it to git

# Database Configuration
DB_PASSWORD=your_secure_database_password_here

# Backend Configuration
SECRET_KEY=generate_with_openssl_rand_hex_32
ENVIRONMENT=production
ADMIN_IP=your.ip.address.here

# OAuth Configuration
# Obtain from Google Cloud Console and Discord Developer Portal
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# VPS Configuration
VPS_IP=your.vps.ip.address
VPS_USER=root

# Docker Registry Configuration
IMAGE_PREFIX=ogdowski
IMAGE_NAME=private

# Application Version
SQUIG_VERSION=0.3.1

# Domain Configuration
DOMAIN=squigleague.com
EMAIL=your@email.com
SSL_EMAIL=your@email.com

# Frontend Configuration
VITE_API_URL=https://squigleague.com/api
