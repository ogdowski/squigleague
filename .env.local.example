# .env.local.example
# Local development environment configuration
# Copy this to .env.local for local development

# Database Configuration
DB_PASSWORD=dev_password_123

# Herald Module
HERALD_ADMIN_KEY=dev_admin_key_123
ADMIN_IP=127.0.0.1

# VPS Configuration (not used in local development)
VPS_IP=localhost
VPS_USER=root

# Docker Registry (for image push/pull)
IMAGE_PREFIX=your_dockerhub_username
IMAGE_NAME=private

# Application Version
SQUIG_VERSION=0.2.1

# Domain Configuration (local development)
DOMAIN=localhost
EMAIL=dev@localhost
