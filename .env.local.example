# .env.local.example
# Local development environment configuration
# Copy this to .env.local for local development

# Database Configuration
DB_PASSWORD=dev_password_123

# Backend Configuration
SECRET_KEY=dev-secret-key-change-in-production-use-openssl-rand-hex-32
ENVIRONMENT=development
ADMIN_IP=127.0.0.1

# OAuth Configuration (optional for initial development)
# Leave empty or obtain credentials from Google Cloud Console / Discord Developer Portal
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=

# VPS Configuration (not used in local development)
VPS_IP=localhost
VPS_USER=root

# Docker Registry (for image push/pull)
IMAGE_PREFIX=ogdowski
IMAGE_NAME=private

# Application Version
SQUIG_VERSION=0.4.11

# Domain Configuration (local development)
DOMAIN=localhost
EMAIL=dev@localhost
SSL_EMAIL=dev@localhost

# Frontend Configuration
VITE_API_URL=http://localhost/api

# Monitoring (optional - get DSN from sentry.io)
SENTRY_DSN=
